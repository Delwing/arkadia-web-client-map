    "@babel/preset-env": "^7.15.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "uglify-js": "^3.17.4",
    "watchify": "^4.0.0"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
