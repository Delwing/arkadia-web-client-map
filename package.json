{
  "name": "arkadia-web-client-map",
  "version": "0.0.1",
  "main": "embedded.js",
  "license": "MIT",
  "scripts": {
    "prepublish": "browserify embedded.js > dist/embedded.js"
  },
  "dependencies": {
    "color-convert": "^2.0.1",
    "mudlet-map-renderer": "^0.0.25"
  },
  "devDependencies": {
