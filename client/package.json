{
  "name": "arkadia-www-client",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prebuild": "mkdirp ../extension/dist/",
    "prepublish": "yarn run build",
    "build": "browserify main.js > ../extension/dist/main.js",
    "watch": "watchify main.js -o ../extension/dist/main.js -v --debug"
  },
  "dependencies": {
    "howler": "^2.2.4",
    "mudlet-map-renderer": "^0.0.25"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "mkdirp": "^3.0.1",
    "uglify-js": "^3.17.4",
    "watchify": "^4.0.0",
    "xml2js": "^0.6.2",
    "xpath-html": "^1.0.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "useBuiltIns": "entry",
                "corejs": "2",
                "targets": {
                  "firefox": "60",
                  "chrome": "61"
                }
              }
            ]
          ]
        }
      ]
    ]
  },
  "files": [
    "dist/bundle.min.js"
  ]
}
