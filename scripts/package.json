{
  "name": "arkadia-www-client",
  "version": "0.0.1",
  "license": "MIT",
  "scripts": {
    "prepublish": "../node_modules/.bin/browserify embedded.js > dist/embedded.js & browserify client/main.js > dist/main.js",
    "watchEmbedded": "../node_modules/.bin/watchify embedded.js -o dist/embedded.js",
    "watchMain": "../node_modules/.bin/watchify client/main.js -o dist/main.js"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/preset-env": "^7.15.0",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "uglify-js": "^3.17.4",
    "watchify": "^4.0.0",
    "xml2js": "^0.6.2",
    "xpath-html": "^1.0.3"
  },
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            [
              "@babel/preset-env",
              {
                "useBuiltIns": "entry",
                "corejs": "2",
                "targets": {
                  "firefox": "60",
                  "chrome": "61"
                }
              }
            ]
          ]
        }
      ]
    ]
  },
  "files": [
    "dist/bundle.min.js"
  ]
}
